# Practice on MySql from W3CSchool

SELECT COUNT(*) AS DistinctCountries FROM (SELECT DISTINCT Country FROM Customers);

SELECT * FROM Customers where country = 'Mexico' and CustomerID BETWEEN 1 AND 58 and CustomerName LIKE '%zuma';

